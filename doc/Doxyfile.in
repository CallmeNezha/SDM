PROJECT_NAME			= "@CMAKE_PROJECT_NAME@"
PROJECT_NUMBER 			= "@SDM_VERSION_MAJOR@.@SDM_VERSION_MINOR@.@SDM_VERSION_PATCH@"

EXTRACT_ALL 			= YES
STRIP_FROM_PATH			= @ROOT_DIR@
INPUT					= @ROOT_DIR@/README.md \
						  @SDM_DIR@
RECURSIVE				= YES
USE_MDFILE_AS_MAINPAGE	= README.md

HAVE_DOT               = YES
CLASS_GRAPH			   = YES

USE_MATHJAX            = YES
MATHJAX_RELPATH        = http://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-AMS-MML_HTMLorMML
